{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "types": ["react", "react-dom"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": false,
    "jsx": "react-jsx"
  },
  "include": ["src-gen/**/*", "src-ts/**/*"]
}
